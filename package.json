{
  "name": "vite-vue2-demo-pc",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "serve": "vite",
    "build": "vite build",
    "serve.pre": "cross-env VITE_APP_ENV_ID=pre npm run serve",
    "serve.prod": "cross-env VITE_APP_ENV_ID=prod npm run serve",
    "build.pre": "cross-env VITE_APP_ENV_ID=pre npm run build",
    "build.prod": "cross-env VITE_APP_ENV_ID=prod npm run build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint": "eslint src --ext .js,.jsx",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "cross-env": "^7.0.3",
    "echarts": "^5.3.3",
    "element-ui": "^2.15.9",
    "path-to-regexp": "^6.2.1",
    "screenfull": "^6.0.2",
    "vue": "^2.6.6",
    "vue-i18n": "8.26.7",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.7.10",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@originjs/vite-plugin-require-context": "^1.0.9",
    "@vue/eslint-config-prettier": "^7.0.0",
    "eslint": "^8.23.0",
    "eslint-define-config": "^1.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.4.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.7.1",
    "sass": "^1.54.8",
    "stylelint": "^14.11.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-order": "^5.0.0",
    "terser": "^5.15.0",
    "vite": "^3.0.7",
    "vite-plugin-vue2": "^2.0.2",
    "vue-eslint-parser": "^9.0.3"
  },
  "lint-staged": {
    "**/*.{js,jsx,}": "yarn run lint",
    "**/*.{js,jsx,less,scss,md}": [
      "prettier --write"
    ]
  }
}
